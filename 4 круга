x = 0
def setup():
    size(650,700)
    frameRate(60)
def draw():
    background(random(0,255), random(0,255), random(0,255))
    fill(random(0,255), random(0,255), random(0,255))
    global x
    translate(width/2, height/2)
    circle(x,x,50)
    circle(-x,x,50)
    circle(x,-x,50)
    circle(-x,-x,50)
    x += 1
